{
  "name": "vue-react-poc",
  "version": "0.0.1",
  "private": false,
  "author": "Guilherme Gambeti",
  "description": "POC to show how we can render VueJs app within ReactJs apps",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "vuejs",
    "poc"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/vue-react-poc.umd.js",
  "module": "./dist/vue-react-poc.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-react-poc.es.js",
      "require": "./dist/vue-react-poc.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier:check": "prettier --check .",
    "typescript:check": "tsc -p . --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@millicast/vue-viewer-plugin": "millicast/vue-viewer-plugin#test-plugin",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/compiler-sfc": "^3.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "veaury": "^2.3.12",
    "vite": "^4.4.9",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.5.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vue/vue3-jest": "^29.2.5",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "prettier": "3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0"
  }
}
